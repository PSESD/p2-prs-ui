<p id="notice"><%= notice %></p>

<h1>Listing District Services</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% #byebug %>

    <% @authorized_entitiy_services.each do |authorized_entity_service| %>
      <% authorized_entity_service.items.each do |service| %>
        <tr>
          <td><%= link_to 'Show', service %></td>
          <td><%= link_to 'Edit', edit_authorized_entity_service_path(service) %></td>
          <td><%= link_to 'Destroy', authorized_entity_service, method: :delete, data: { confirm: 'Are you sure?' } %></td>
        </tr>
      <% end %>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Service', new_district_service_path %>
