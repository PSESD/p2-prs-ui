<p id="notice"><%= notice %></p>

<h1>Listing District Services</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @services.each do |service| %>
      <tr>
        <td><%= link_to 'Show', district_services_path(service) %></td>
        <td><%= link_to 'Edit', edit_district_service_path(district_id: service.districtId, id: service.id) %></td>
        <td><%= link_to 'Destroy', district_service_path(district_id: service.districtId, id: service.id), method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Service', new_district_service_path %>
